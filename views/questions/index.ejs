<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/css2?family=Baloo+Da+2:wght@500;800&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="../styles/main.css">
    <title>Document</title>
</head>
<body>
    <header>
       <h1>list of questions</h1> 
    </header>
    

    <% if (questions.length) { %>
        <% questions.forEach((question) => { %>
            <a href="/questions/<%= question._id %>">
                <h4><%= question.question %></h4>
            </a>
        <% } )%>
        <% } else { %>
            <h4>No Authors found</h4>
    <% } %>
</body>
</html>
